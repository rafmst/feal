{
  "name": "feal",
  "version": "0.2.0",
  "description": "Feal is an API boilerplate that uses Koa with Typescript and MongoDB.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "prod": "node .",
    "tsc": "tsc",
    "postinstall": "npm run tsc"
  },
  "keywords": [
    "rest",
    "api",
    "typescript"
  ],
  "author": "Rafael Morais <rafaelcanical@gmail.com>",
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false,
    "parser": "typescript",
    "spaceAfterFunction": true
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^9.1.0",
    "mongoose": "^5.9.28"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cors": "0.0.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.0.27",
    "nodemon": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  }
}
